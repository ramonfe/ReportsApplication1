      <Query>
        <DataSourceName>EficienConnectionString</DataSourceName>
        <CommandText>SELECT        FBU_s.FBU, FBU_s.Descripcion AS fbu_desc, Departamentos.Departamento, Departamentos.Descripcion AS depDesc, CentrosTrabajos.CentroTrabajo, 
                         CentrosTrabajos.Descripcion
FROM            ((CentrosTrabajos INNER JOIN
                         Departamentos ON CentrosTrabajos.Departamento = Departamentos.Departamento) INNER JOIN
                         FBU_s ON CentrosTrabajos.FBU = FBU_s.FBU)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
